{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "warn",
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
    },
    "parserOptions": {
      "sourceType": "module"
    }
}